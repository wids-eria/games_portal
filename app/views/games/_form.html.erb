<%= simple_form_for @game do |f| %>
  <%= f.input :name %>
  <%= f.input :path %>

  <%= f.simple_fields_for :survey do |s| %>
    <%= s.input :url %>
  <% end %>

  <%= f.input :about, as: :text%>
  <%= f.input :description, as: :text %>
  <%= f.input :resources, as: :text %>
  <%= f.input :image %>
  <%= f.input :localpath%>
  <%= f.input :file %>
  <%= f.button :submit%>
<% end %>